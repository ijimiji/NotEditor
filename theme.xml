<theme>
   <font>
       <family>FantasqueSansMono Nerd Font</family> 
       <size>20</size>
   </font>
   <colors>
      <black>#2E3440</black>
      <blue>#5E81AC</blue>
      <gray>#4C566A</gray>
      <green>#A3BE8C</green>
      <orange>#D08770</orange>
      <purple>#B48EAD</purple>
      <red>#BF616A</red>
      <white>#ECEFF4</white>
      <yellow>#EBCB8B</yellow>
   </colors>
   <keywords>
      <builtin>
         <colors>
            <background>black</background>
            <foreground>blue</foreground>
         </colors>
         <list>
            <element>class</element>
            <element>define</element>
            <element>def</element>
            <element>class</element>
            <element>lambda</element>
            <element>import</element>
            <element>export</element>
            <element>print</element>
            <element>from</element>
            <element>include</element>
            <element>return</element>
         </list>
      </builtin>
      <default>
         <colors>
            <background>black</background>
            <foreground>white</foreground>
         </colors>
         <list />
      </default>
      <error>
         <colors>
            <background>black</background>
            <foreground>red</foreground>
         </colors>
         <list>
            <element>error</element>
            <element>catch</element>
            <element>throw</element>
            <element>exception</element>
         </list>
      </error>
      <special>
         <colors>
            <background>black</background>
            <foreground>purple</foreground>
         </colors>
         <list>
            <element>using</element>
            <element>while</element>
            <element>if</element>
            <element>else</element>
            <element>\-</element>
            <element>\+</element>
            <element>&lt;.*&gt;</element>
         </list>
      </special>
      <type>
         <colors>
            <background>black</background>
            <foreground>green</foreground>
         </colors>
         <list>
            <element>int</element>
            <element>string</element>
            <element>char</element>
            <element>bool</element>
            <element>boolean</element>
            <element>double</element>
            <element>float</element>
            <element>true</element>
            <element>false</element>
            <element>True</element>
            <element>False</element>
         </list>
      </type>
      <warning>
         <colors>
            <background>black</background>
            <foreground>yellow</foreground>
         </colors>
         <list>
            <element>warning</element>
            <element>".*"</element>
         </list>
      </warning>
   </keywords>
</theme>
